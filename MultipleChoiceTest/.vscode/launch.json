{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ThiTracNghiem",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/CMake/ThiTracNghiem.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/CMake",
            "environment": [],
            "externalConsole": true,

            "MIMode": "gdb",
            "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",

            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
